{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run SAPFiori1-HTML5Module",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/sapfiori1-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/comyashui5HTML5Module/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/HTML5Module",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/SAPFiori1/HTML5Module\"}"
      },
      "envFile": "${workspaceFolder}/HTML5Module/.env1",
      "preLaunchTask": "Build SAPFiori1"
    }
  ]
}